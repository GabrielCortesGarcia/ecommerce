{
  "name": "styleshop-ecommerce",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "E-commerce completo de tienda de ropa con React, TypeScript y Tailwind CSS",
  "author": "StyleShop Team",
  "keywords": ["ecommerce", "fashion", "react", "typescript", "tailwind"],
  "scripts": {
